              "options": [
                {
                  "text": {
                    "type": "plain_text",
                    "text": "Yes",
                    "emoji": true
                  },
                  "value": "Yes"
                },
                {
                  "text": {
                    "type": "plain_text",
                    "text": "No",
                    "emoji": true
                  },
                  "value": "No"
                },
                {
                  "text": {
                    "type": "plain_text",
                    "text": "Waiting for the company to revert",
                    "emoji": true
                  },
                  "value": "Waiting for the company to revert"
                }
              ],
              "action_id": "movedToNextRoundOrAnOffer"
