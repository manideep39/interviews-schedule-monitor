        {
            "block_id": "whatWentWrong",
            "type": "input",
            "element": {
              "type": "plain_text_input",
              "multiline": true,
              "action_id": "whatWentWrong",
              "placeholder": {
                "type": "plain_text",
                "text": " ",
                "emoji": true
              }
            },
            "label": {
              "type": "plain_text",
              "text": "What went wrong?",
              "emoji": true
            }
