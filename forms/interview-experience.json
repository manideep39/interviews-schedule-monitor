              "placeholder": {
                "type": "plain_text",
                "text": " ",
                "emoji": true
              }
            },
            "label": {
              "type": "plain_text",
              "text": "In case you didn't get the offer and you have to re-take the interview, what would you want to change?",
              "emoji": true
            }
          },
        {
            "block_id": "assignmentOrRelevantDocLink",
            "type": "input",
            "element": {
              "type": "plain_text_input",
              "action_id": "assignmentOrRelevantDocLink",
              "placeholder": {
                "type": "plain_text",
                "text": "Please upload the files to google drive and share the link",
                "emoji": true
              }
            },
            "label": {
              "type": "plain_text",
              "text": "Upload assignments/relevant document link here, if any",
              "emoji": true
            }
        }
